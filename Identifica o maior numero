num1 = int(input('digite o primeiro numero inteiro:'))
num2 = int(input('digite o segundo numero inteiro:'))
num3 = int(input('digite o terceiro numero inteiro:'))

if num1 < num2:
    if num1 < num3:
        min = num1
    else:
        min = num3 
elif num2 < num3:  
    min = num2 
else:
    min = num3
print('O menor numero Ã©' ,min)
